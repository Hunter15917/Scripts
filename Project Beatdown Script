 for i,v in pairs(getconnections(game:GetService("ScriptContext").Error)) do 
        v:Disable()
 end

local wrksp = game:GetService("Workspace");
local StarterGui = game:GetService("StarterGui")
local UIS = game:GetService("UserInputService")

local Player = game.Players.LocalPlayer

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Hunter15917/Scripts/main/MainGUI.md"))()
local venyx = library.new("Sparrow", 5013109572)
 
 
-- themes
       local themes = {
       Background = Color3.fromRGB(0, 0, 0),
        Glow = Color3.fromRGB(159, 0, 255),
        Accent = Color3.fromRGB(47, 11, 227),
        LightContrast = Color3.fromRGB(17, 17, 223),
        DarkContrast = Color3.fromRGB(12, 12, 12),  
        TextColor = Color3.fromRGB(255, 255, 255)
}
 
 
-- first page
local page = venyx:addPage("yerrrr", 5012544693)
local section1 = page:addSection("Auto Farms")
local section2 = page:addSection("Extras")

-- variables
local Character = Player.Character 
local HumanoidRootPart = Character.HumanoidRootPart
local Camera = wrksp.CurrentCamera;

local cantoggle = true

local function NOTIFICATION(Title, value)
   spawn(function()
    if cantoggle == true then
        venyx:Notify(Title, value)  
      end
   end)
end

local ts = game:GetService("TweenService")
local function Tween(part, endpos, speed)
    if part and endpos then
        local Time = (endpos - part.Position).magnitude/speed
        local Info = TweenInfo.new(Time, Enum.EasingStyle.Linear)
        local Tween = ts:Create(part,Info,{CFrame = CFrame.new(endpos.X,endpos.Y,endpos.Z)})
        Tween:Play()
        wait(Time)
    end
end


local mt = getrawmetatable(game)
make_writeable(mt)
local namecall = mt.__namecall
mt.__namecall = newcclosure(function(self, ...)
    local method = getnamecallmethod()
    local args = {...}

    if method == "InvokeServer" and tostring(self) == "DashEvent" or method == "FireServer" and tostring(self) == "RunEvent" then
        if Toggle2 == true and cantoggle == true then
         print("Blocked: "..tostring(self))
         return
       end
    end
    return namecall(self, table.unpack(args))
end)

local Rm, Index, NIndex, NCall, Caller = getrawmetatable(game), getrawmetatable(game).__index, getrawmetatable(game).__newindex, getrawmetatable(game).__namecall, checkcaller or is_protosmasher_caller
setreadonly(Rm, false)


Rm.__newindex = newcclosure(function(self, Meme, Value)
    if Caller() then return NIndex(self, Meme, Value) end 
    if tostring(self) == "HumanoidRootPart" or tostring(self) == "Torso" then 
        if Meme == "CFrame" and self:IsDescendantOf(Character) then 
            return true 
        end
    end
    return NIndex(self, Meme, Value)
end)
setreadonly(Rm, true)



local MoneyFarmDown = false
local AtPizzaMan = false
local AfterGettingJob = false

section1:addToggle("Money Farm", nil, function(r)
if r then
          if Character.Torso.CanCollide == true then
             NOTIFICATION("NOTIFICATION", "TURN ON NOCLIP")
         elseif Character.Torso.CanCollide == false  then
             NOTIFICATION("NOTIFICATION", "Turned ON")
         end
          Toggle1 = true
          else
          Toggle1 = false
          local ts = game:GetService("TeleportService")
          ts:Teleport(game.PlaceId, Player)
          game:GetService("Players").LocalPlayer.OnTeleport:Connect(function(State)
       syn.queue_on_teleport([[
          
        ]])
    end)
          NOTIFICATION("NOTIFICATION", "Turned OFF")
      end
  while Toggle1 do
    pcall(function() 
   if Toggle1 and Toggle2 then
     if Character:FindFirstChild("Head") then
         Character.Head:Destroy()
     end
      if Character.HumanoidRootPart:FindFirstChild("NoGravity") == nil then
      local NoGrav = Instance.new("BodyVelocity")
      NoGrav.Name = "NoGravity"
      NoGrav.MaxForce = Vector3.new(0, 100000000, 0)
      NoGrav.P = 90000000
      NoGrav.Velocity = Vector3.new(0, 0, 0)
      NoGrav.Parent = HumanoidRootPart 
      end
       if MoneyFarmDown == false then
            Tween(Character.HumanoidRootPart, Vector3.new(Character.HumanoidRootPart.Position.X,-12,Character.HumanoidRootPart.Position.Z), 50)
            MoneyFarmDown = true
        end
        if not Character:FindFirstChild("Mission") and MoneyFarmDown == true and AtPizzaMan == false then
             Tween(Character.HumanoidRootPart, wrksp.Missions.PizzaDelivery.Position - Vector3.new(0,15,10), 125)
             AtPizzaMan = true
         elseif not Character:FindFirstChild("Mission") and MoneyFarmDown == true and AtPizzaMan == true then  
            Tween(Character.HumanoidRootPart, wrksp.Missions.PizzaDelivery.Position - Vector3.new(0,-2,10), 50)
            fireclickdetector(wrksp.Missions.PizzaDelivery.ClickDetector)
            repeat game:GetService("RunService").Stepped:Wait() until Character:FindFirstChild("Mission")
            MoneyFarmDown = false
          end
        if Character:FindFirstChild("Mission") and MoneyFarmDown == true and AtPizzaMan == true then
           AtPizzaMan = false
           
           if Character:FindFirstChild("Mission").Location.Value == 1 then
             Tween(Character.HumanoidRootPart, Vector3.new(-378.1214599609375, -12, -467.05682373046875), 125)
             Tween(Character.HumanoidRootPart, Vector3.new(-378.1214599609375, 3.4999992847442627, -467.05682373046875), 50)
             repeat game:GetService("RunService").Stepped:Wait() until Character:FindFirstChild("Mission") == nil
             MoneyFarmDown = false
           elseif Character:FindFirstChild("Mission").Location.Value == 4 then
              Tween(Character.HumanoidRootPart, Vector3.new(-92.04825592041016, -12, -602.7265625), 125)
              Tween(Character.HumanoidRootPart, Vector3.new(-92.04825592041016, 3.4999992847442627, -602.7265625), 50)
              repeat game:GetService("RunService").Stepped:Wait() until Character:FindFirstChild("Mission") == nil
              MoneyFarmDown = false
          
           elseif Character:FindFirstChild("Mission").Location.Value == 3 then
              Tween(Character.HumanoidRootPart, Vector3.new(-62.05238723754883, -12, -79.0844497680664), 125)
              Tween(Character.HumanoidRootPart, Vector3.new(-62.05238723754883, 2.9999992847442627, -79.0844497680664), 50)
              repeat game:GetService("RunService").Stepped:Wait() until Character:FindFirstChild("Mission") == nil
              MoneyFarmDown = false  
          
            elseif Character:FindFirstChild("Mission").Location.Value == 2 then
              Tween(Character.HumanoidRootPart, Vector3.new(304.9508361816406, -12, -176.9788818359375), 125)
              Tween(Character.HumanoidRootPart, Vector3.new(304.9508361816406, 3.0499989986419678, -176.9788818359375), 50)
              repeat game:GetService("RunService").Stepped:Wait() until Character:FindFirstChild("Mission") == nil
              MoneyFarmDown = false  
              end
           end
        end
    end)
    game:GetService("RunService").Stepped:Wait()
   end
end)


section2:addToggle("No Clip", nil, function(r)
if r then
          Toggle2 = true
          NOTIFICATION("NOTIFICATION", "Turned ON")
          else
          Toggle2 = false
          NOTIFICATION("NOTIFICATION", "Turned OFF")
      end
  while Toggle2 do
    pcall(function() 
       Character.Head.CanCollide = false 
       Character.Torso.CanCollide = false 
    end)
    game:GetService("RunService").Stepped:Wait()
   end
end)


