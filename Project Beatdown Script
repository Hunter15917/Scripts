section1:addButton("Delete Gui", function()

NOTIFICATION("NOTIFICATION","Successfully Deleted")
venyx:deleteui()
spawn(function()
cantoggle = false
Toggle1 = false
Toggle2 = false
Toggle3 = false
Toggle4 = false
Toggle5 = false
Toggle6 = false
end)   
    spawn(function()
       wait(5)
        game.CoreGui.Sparrow:Destroy() 
      end)
   end)

